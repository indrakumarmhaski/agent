    "@types/uuid": "9.0.1",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "axios-mock-adapter": "1.21.1",
    "dotenv": "16.0.1",
    "eslint": "8.19.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.27.5",
