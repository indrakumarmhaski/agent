    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "merge2": "1.4.1",
    "prettier": "2.2.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.8.2",
    "typescript": "4.7.4",
    "vinyl-sourcemaps-apply": "0.2.1"
